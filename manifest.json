{
	"manifest_version": 2,

	"name": "ZUZU-Magic",
	"description": "ZG<=>UNI for all",
	"version": "1.0.1",
	"icons": {
		"16": "icon/icon16.png",
		"48": "icon/icon48.png",
		"128": "icon/icon128.png"
	},
	"content_scripts": [ {
		"js": [
			"content-scripts/detector.js",
			"content-scripts/uni512zg1.js",
			"content-scripts/zg12uni51.js",
			"content-scripts/zuzu.js"
		],
		"matches": [ "*://*/*" ],
		"run_at": "document_idle"
	}],
	"options_page":"options/options.html",
	"permissions": [
		"storage",
		"clipboardRead",
		"clipboardWrite"
	]
}
